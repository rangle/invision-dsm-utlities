{
  "name": "invision-dsm-utils",
  "version": "0.1.0",
  "description": "Utilities to integrate InVision DSM with front-end.",
  "scripts": {
    "build": "babel src --out-dir lib",
    "demo:transform": "babel-node -- ./src/transform-to-theme -s ./mock/_style-data.json -d ./src/theme.dsm.js",
    "demo:download": "babel-node -- ./src/download-design-tokens -s 'https://rangle.invisionapp.com/dsm-export/rangle-io/where-van-gogh/style-data.json?exportFormat=lookup&key=Hk4MnZ1bU' -d ./src/_style-data.json"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "bin":{
    "transform-to-theme": "./lib/transform-to-theme.js",
    "download-design-tokens": "./lib/download-design-tokens.js"
  },
  "author": "Chris DePaul",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-codegen": "^3.1.0",
    "minimist": "^1.2.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.8.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3"
  }
}
