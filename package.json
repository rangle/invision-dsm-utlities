{
  "name": "invision-dsm-utils",
  "version": "0.1.0",
  "description": "Utilities to integrate InVision DSM with front-end codebase.",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "pretest": "npm run build",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage",
    "lint": "eslint --ext js,ts src test",
    "check-types": "tsc --noEmit",
    "prettier": "prettier --write \"{src,test}/**/*.{js,ts}\"",
    "prettier:check": "prettier --check \"{src,test}/**/*.{js,ts}\"",
    "prepare": "npm run clean && npm run check-types && npm run prettier:check && npm run lint && npm run test"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "bin": {
    "invision-dsm-utils": "./dist/cli.js"
  },
  "files": ["/dist"],
  "main": "./dist/cli.js",
  "author": "Chris DePaul",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-codegen": "^3.1.0",
    "commander": "^4.1.0",
    "cosmiconfig": "^6.0.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/babel__core": "^7.1.3",
    "@types/jest": "^25.1.1",
    "@types/node": "^13.5.0",
    "@types/styled-system": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "jest": {
    "testRegex": "(/test/.*\\.spec\\.[tj]s)$"
  }
}
