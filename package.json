{
  "name": "invision-dsm-utils",
  "version": "0.1.0",
  "description": "Utilities to integrate InVision DSM with front-end.",
  "scripts": {
    "build": "npm run build-ts",
    "build-ts": "tsc",
    "demo:download": "ts-node -- ./src/cli download --url 'https://rangle.invisionapp.com/dsm-export/rangle-io/where-van-gogh/style-data.json?exportFormat=lookup&key=Hk4MnZ1bU' --out-file ./src/_style-data.json",
    "demo:transform": "ts-node -- ./src/cli transform --in-file ./mock/_style-data.json --out-file ./src/theme.dsm.js",
    "demo:transform-default": "ts-node -- ./src/cli transform --in-file ./mock/_style-data.json"

  },
  "engines": {
    "node": ">=8.10.0"
  },
  "bin": {
    "invision-utils-cli": "./dist/cli.js"
  },
  "author": "Chris DePaul",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-codegen": "^3.1.0",
    "commander": "^4.1.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@types/babel__core": "^7.1.3",
    "@types/node": "^13.5.0",
    "@types/styled-system": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
